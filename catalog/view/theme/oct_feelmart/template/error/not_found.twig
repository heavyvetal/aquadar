{{ header }}
<div id="error-not-found" class="container flex-container">
	<div class="content-top-box">{{ content_top }}</div>
			<div class="fm-content fm-404-content h-100 text-center">
				<h1 class="fm-main-title fm-404-content-title text-center">{{ heading_title }}</h1>
				{% if (oct_404_image is defined and oct_404_image is not empty) %} 
				<div class="fm-404-content-image"><img class="img-fluid" src="{{ oct_404_image }}" /></div>
				{% endif %}
				<div class="fm-404-content-text">
					<p>{{ text_error }}</p>
					<a href="{{ continue }}" class="fm-btn fm-btn-blue mt-3">{{ button_continue }}</a>
				</div>
			</div>
	{{ content_bottom }}
</div>
{{ footer }}