</head>
<body class="padding-{{ sidebar_position }}">
<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TSPMR8C" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
<div id="fm_sidebar_overlay"></div>
<div id="fm_sidebar">
    <div class="fm_sidebar-title d-flex alig-items-center justify-content-between">
      <span class="fm_sidebar-title-text">{{ oct_menu }}</span>
      <span class="fm_sidebar-title-close"></span>
    </div>
    <div id="fm_sidebar_content" class="fm_sidebar-content"></div>
</div>
{{ oct_sidebar_mobile }}
<nav id="top" class="top d-none d-lg-block">
	<div class="container d-flex justify-content-between">
	  	{% if header_informations is defined and header_informations is not empty %}
		<ul class="info-list list-unstyled d-flex">
			{% for header_link in header_informations %}
			<li class="info-item"><a href="{{ header_link.href }}" class="info-link">{{ header_link.title }}</a></li>
			{% endfor %}
		</ul>
		{% endif %}

		<div class="left-side nav d-flex">
			<div class="cell"><a href="{{ text_top_link1 }}">{{ text_top_row1 }}</a></div>
			<div class="cell"><a href="{{ text_top_link2 }}">{{ text_top_row2 }}</a></div>
			<div class="cell"><a href="{{ text_top_link3 }}">{{ text_top_row3 }}</a></div>
		</div>

		<div class="right-side nav d-flex">
			{{ language }}
			{{ currency }}
			<div class="dropdown cell" id="fm-account-dropdown">
				{% if logged %}
				<a href="{{ account }}" title="{{ text_account }}" class="btn btn-user btn-link dropdown-toggle btn-block h-100 d-flex align-items-center" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{ text_account }} <i class="fas fa-chevron-down" ></i></a>
				<ul class="dropdown-menu nav-dropdown-menu user-dropdown-menu">
		            <li class="dropdown-item my-dropdown-item"><a href="{{ account }}">{{ text_account }}</a></li>
		            <li class="dropdown-item my-dropdown-item"><a href="{{ order }}">{{ text_order }}</a></li>
		            <li class="dropdown-item my-dropdown-item"><a href="{{ transaction }}">{{ text_transaction }}</a></li>
		            <li class="dropdown-item my-dropdown-item"><a href="{{ download }}">{{ text_download }}</a></li>
		            <li class="dropdown-item my-dropdown-item"><a href="{{ logout }}">{{ text_logout }}</a></li>
				</ul>
				{% else %}
				<a href="javascript:;" onclick="octPopupLogin();" title="{{ text_account }}" class="btn btn-user btn-link btn-block h-100 d-flex align-items-center" role="button" aria-haspopup="true">
					<img src="/catalog/view/theme/oct_feelmart/img/icon_profile.svg" alt="">
					<span>{{ text_account }}</span>
				</a>
				{% endif %}
	        </div>
		</div>
	</div>
</nav>
<header>
  <div class="container">
    <div class="row align-items-center">
		{% if logo %}
    	<div id="logo" class="col-6 col-md-2 col-lg-3 col-xl-2">
			{% if oct_home is defined and oct_home %}
				<div class="fm-logo">
					<a href="/">
						<img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-fluid" />
					</a>
				</div>
				<div class="address_title">{{ address_title }}</div>
			{% else %}
			<div class="fm-logo">
			<a href="{{ home }}">
				<img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-fluid" />
			</a>
			</div>
			{% endif %}
		</div>
		{% else %}
		<h1><a href="{{ home }}">{{ name }}</a></h1>
		{% endif %}
		{% if oct_feelmart_data.contact_address[oct_lang_id] is defined and oct_feelmart_data.contact_address[oct_lang_id] %}
		<div class="d-none d-md-flex col-md-7 col-lg-6 col-xl-6 fm-header-address">
	        <div class="header-dropdown-box">
				<a class="header-dropdown-toggle btn btn-header btn-link d-flex align-items-center h-100" href="javascript:;" role="button">

					<span class="address_title">{{ address_title }}</span>

					<i class="fas fa-map-marker-alt"></i>
					<span>{{ oct_feelmart_data.contact_address[oct_lang_id] }}</span>
				</a>
				<div class="header-dropdown text-center">
					<div class="header-dropdown-inner fm-header-address-dropdown-inner">
						<div class="header-dropdown-title">{{ oct_our_address }}:</div>
					    <div class="header-dropdown-address">{{ oct_feelmart_data.contact_address[oct_lang_id] }}</div>
					    {% if contact_map is defined and contact_map %}
					    <div class="header-dropdown-map">
					    	<div id="oct-contact-map" class="not_in"></div>
					    </div>
					    <script>
					    	$('.header-dropdown-toggle').on('mouseenter mouseleave', function() {
						    	octShowMap('{{ contact_map }}');
							});
					    </script>
					    {% endif %}
				    </div>
				</div>
			</div>
		</div>
		{% endif %}
		{% if oct_contact_opens is defined and oct_contact_opens is not empty %}
		<div class="fm-header-shedule d-none col-xl-2 d-xl-flex justify-content-center">
          <div class="header-dropdown-box">
            <a class="header-dropdown-toggle btn btn-header btn-link d-flex align-items-center h-100" href="javascript:;" role="button">
{#              <i class="fas fa-clock"></i>#}
				<img src="/catalog/view/theme/oct_feelmart/img/icon_clock.svg" alt="">
              <span>{{ oct_working_hours }}</span>
              <i class="fas fa-chevron-down"></i>
            </a>
            <div class="header-dropdown header-dropdown-shedule text-center">
              <div class="header-dropdown-inner">
                <div class="header-dropdown-title header-dropdown-title-bold">{{ oct_working_hours_shop }}</div>
                <ul class="list-unstyled">
                	{% for oct_contact_open in oct_contact_opens %}
					<li>{{ oct_contact_open }}</li>
					{% endfor %}
                </ul>
              </div>
            </div>
          </div>
        </div>
		{% endif %}
		{% if oct_contact_telephones is defined and oct_contact_telephones is not empty %}
		<div class="col-6 col-md-3 col-xl-2 d-flex fm-header-phones">
          <div class="fm-phone-box d-flex align-items-center">
            <div class="fm-phone-img">
{#              <i class="fas fa-mobile-alt"></i>#}
				<img src="/catalog/view/theme/oct_feelmart/img/icon_phone.svg" alt="icon_phone">
            </div>
            <div class="fm-phone-content text-right">
            {% if oct_contact_telephones|length > 1 %}
              <div class="header-dropdown-box btn-link dropdown-toggle align-items-center">
				  <div class="phone-number-row">
					  <a href="tel:{{ oct_contact_telephones[0]|replace({" " : "", "-" : "", "(" : "", ")" : ""}) }}" class="phoneclick">{{ oct_contact_telephones[0] }}</a>
				  </div>
				  <div class="phone-number-row">
					  <a href="tel:{{ oct_contact_telephones[1]|replace({" " : "", "-" : "", "(" : "", ")" : ""}) }}" class="phoneclick">{{ oct_contact_telephones[1] }}</a>
				  </div>
              </div>
              {% else %}
				<a class="btn btn-link dropdown-toggle d-flex align-items-center" href="tel:{{ oct_contact_telephones[0]|replace({" " : "", "-" : "", "(" : "", ")" : ""}) }}" role="button" id="us-phones-dropdown"><span>{{ oct_contact_telephones[0] }}</span></a>
				{% endif %}
            </div>
          </div>
        </div>
        {% endif %}
    </div>
  </div>
</header>
<div class="menu-row sticky-top">
	<div class="container">
		<div class="row">
			{{ menu }}
	        <div class="col-lg-9 fm-menu-buttons-list-col d-flex justify-content-between">
		        {{ search }}
		        <div class="fm-menu-buttons-list d-flex">
		        	{% if (oct_feelmart_data.header_product_views is defined and oct_feelmart_data.header_product_views) and (product_views_count is defined and product_views_count) %}
		            <button title="{{ fixed_bar_views }}" type="button" class="fm-menu-buttons-item fm-menu-buttons-viewed" onclick="fmSidebar('{{ oct_product_views }}', 'viewed');">
			            <span class="fm-menu-buttons-icon">
			                <img src="catalog/view/theme/oct_feelmart/img/viewed-icon.svg" alt="viewed-icon">
			                <span class="fm-menu-buttons-index fm-menu-viewed-index">{{ product_views_count }}</span>
			            </span>
		            </button>
		            {% endif %}
		            <button title="{{ fixed_bar_wishlist }}" onclick="location.href='{{ wishlist_link }}';" type="button" class="fm-menu-buttons-item fm-menu-buttons-wishlist">
			            <span class="fm-menu-buttons-icon">
			                <img src="catalog/view/theme/oct_feelmart/img/heart-icon.svg" alt="heart-icon">
			                <span class="fm-menu-buttons-index fm-menu-wishlist-index">{{ wishlist_total }}</span>
			            </span>
		            </button>
		            {{ cart }}
		        </div>
	        </div>
		</div>
	</div>
</div>
